    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service serve --mode dev",
        "local-build": "vue-cli-service build --mode build",
        "build": "vue-cli-service build --mode prod",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "apollo-boost": "^0.4.0",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link": "^1.2.14",
        "apollo-link-http": "^1.5.17",
        "axios": "^0.19.2",
        "core-js": "^2.6.5",
        "font-awesome": "^4.7.0",
        "graphql": "^14.6.0",
        "graphql-tag": "^2.10.3",
        "immer": "^6.0.5",
        "js-cookie": "^2.2.1",
        "less": "^3.11.1",
        "less-loader": "^5.0.0",
        "normalize.css": "^8.0.1",
        "quill": "^1.3.7",
        "quill-image-drop-module": "^1.0.3",
        "quill-image-extend-module": "^1.1.2",
        "quill-image-resize-module": "^3.0.0",
        "vue": "^2.6.10",
        "vue-apollo": "^3.0.3",
