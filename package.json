{
    "name": "messageRouteApp-client",
    "version": "2.0.2",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service serve --mode dev",
        "dev-light": "vue-cli-service serve --mode devA",
        "prebuild": "echo '打包开始'",
        "postbuild": "echo '打包完毕'",
        "//": "本行为注释：并行执行多个script脚本  npm run build:light & npm run build:dark",
        "build": "npm-run-all --parallel build:dark build:light",
        "build:dark": "vue-cli-service build --mode prod",
        "build:light": "vue-cli-service build --mode prodA",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "apollo-boost": "^0.4.9",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-client": "^2.6.10",
        "apollo-link": "^1.2.14",
        "apollo-link-http": "^1.5.17",
        "axios": "^0.19.2",
        "core-js": "^2.6.12",
        "create-keyframe-animation": "^0.1.0",
        "echarts-gl": "^2.0.1",
        "font-awesome": "^4.7.0",
        "graphql": "^14.7.0",
        "graphql-tag": "^2.11.0",
        "immer": "^6.0.9",
        "js-cookie": "^2.2.1",
        "less": "^3.13.1",
        "less-loader": "^5.0.0",
        "mammoth": "^1.4.16",
        "quill": "^1.3.7",
        "quill-image-drop-module": "^1.0.3",
        "quill-image-extend-module": "^1.1.2",
        "quill-image-resize-module": "^3.0.0",
        "vue": "^2.6.12",
        "vue-apollo": "^3.0.5",
        "vue-pdf": "^4.2.0",
        "vue-quill-editor": "^3.0.6",
        "vue-router": "^3.4.9",
        "vuedraggable": "^2.24.3",
        "xlsx": "^0.16.9"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.12.1",
        "@vue/cli-plugin-eslint": "^3.12.1",
        "@vue/cli-service": "^3.12.1",
        "babel-eslint": "^10.1.0",
        "compression-webpack-plugin": "^6.1.1",
        "echarts": "^4.9.0",
        "element-ui": "^2.14.1",
        "eslint": "^5.16.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-vue": "^5.2.3",
        "npm-run-all": "^4.1.5",
        "style-resources-loader": "^1.4.1",
        "svg-sprite-loader": "^5.2.1",
        "vue-cli-plugin-style-resources-loader": "^0.1.4",
        "vue-template-compiler": "^2.6.12",
        "vuex": "^3.6.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": 1,
            "no-debugger": 2
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
